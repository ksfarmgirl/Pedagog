	"version": "0.2.0",
	"configurations": [
		{
			"type": "mock",
			"request": "launch",
			"name": "Debug test.py",
			"program": "${workspaceFolder}/test.py",
			"stopOnEntry": true,
			"trace": false
		},
		{
			"type": "mock",
			"request": "attach",
			"name": "Attach test.py",
			"program": "${workspaceFolder}/test.py",
			"stopOnEntry": true,
			"trace": false
		},
		{
			"name": "Python Extension",
			"type": "python",
			"request": "launch",
			"program": "${workspaceFolder}/test.py",
			"stopOnEntry": true,
			"console": "integratedTerminal",
			"justMyCode": false
		}
	]
}
